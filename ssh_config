# version 0004
# for IdentitiesOnly yes see:
# https://stackoverflow.com/a/11251797
Host github.com
    HostName github.com
    User git
    IdentityFile ~/.ssh/id_rsa
    IdentitiesOnly yes
Host github-WORK
    HostName github.com
    User git
    IdentityFile ~/.ssh/work_rsa
    IdentitiesOnly yes
Host github-ZillyHuhn
    HostName github.com
    User git
    IdentityFile ~/.ssh/ZillyHuhn/id_rsa
    IdentitiesOnly yes
Host aur.archlinux.org
    IdentityFile ~/.ssh/chiller@debian/id_rsa
    User aur

Include config.d/*
